{
  "name": "gateway-api",
  "version": "1.0.0",
  "description": "Initial gateway api repository",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "tsc && node dist/server.js",
    "dev": "nodemon --config nodemon.json",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/server.ts",
    "ts-node-dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "_moduleAliases": {
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@repositories": "dist/repositories",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twoMatches/gateway-api.git"
  },
  "author": "Mahesh Mahadevan",
  "license": "TwoMatches",
  "bugs": {
    "url": "https://github.com/twoMatches/gateway-api/issues"
  },
  "homepage": "https://github.com/twoMatches/gateway-api#readme",
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "axios": "^0.23.0",
    "axios-logger": "^2.5.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-jwt-authz": "^2.4.1",
    "express-preconditions-addl": "^1.0.2",
    "express-validation": "^3.0.8",
    "helmet": "^4.6.0",
    "jwks-rsa": "^2.0.5",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.29",
    "slugify": "^1.6.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^40.4.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "prisma": "^3.2.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
